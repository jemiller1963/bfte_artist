<div class="mb-6">
  <%= f.label :email, class:"label" %>
  <%= f.email_field :email, autocomplete: "email", class:"input" %>
</div>

<div class="mb-6">
  <%= f.label 'First Name', class:"label" %>
  <%= f.text_field :fname, class:"input" %>
</div>

<div class="mb-6">
  <%= f.label 'Last Name', class:"label" %>
  <%= f.text_field :lname, class:"input" %>
</div>

<div class="mb-6">
  <%= f.label 'Studio Name', class:"label" %>
  <%= f.text_field :studio, class:"input" %>
</div>

<div class="mb-6">
  <%= f.label 'City', class:"label" %>
  <%= f.text_field :city, class:"input" %>
</div>

<div class="md-6">
  <%= f.label 'State', class: 'label' %>
  <%= f.select(:state, show_states, { include_blank: true, class: 'border py-2 px-3 text-grey-darkest' }) %>
</div>

<div class="mb-6">
  <%= f.label 'Description', class:"label" %>
  <%= f.text_area(:desc, cols: 2, rows: 4, class: 'input') %>
</div>

<div class="mb-6">
  <%= f.label 'Webpage', class:"label" %>
  <%= f.text_field :webpage, class:"input" %>
</div>

<div class="mb-6">
  <%= f.label 'Facebook', class:"label" %>
  <%= f.text_field :facebook, class:"input" %>
</div>

<div class="mb-6">
  <%= f.label 'Instagram', class:"label" %>
  <%= f.text_field :instagram, class:"input" %>
</div>
